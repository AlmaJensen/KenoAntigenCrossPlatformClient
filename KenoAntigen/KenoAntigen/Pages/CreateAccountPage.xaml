<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KenoAntigen.Pages.CreateAccountPage">
  <ContentPage.Content>
    <StackLayout>
      <BoxView BackgroundColor="Transparent" HeightRequest="80" />
      <Entry Text="{Binding EmpireName}" Placeholder="Empire Name" />
      <Entry Text="{Binding EmailAddress}" Placeholder="Email Address" />
      <Button Text ="Submit" Command="{Binding SubmitCommand}" />
    </StackLayout>
  </ContentPage.Content>
</ContentPage>